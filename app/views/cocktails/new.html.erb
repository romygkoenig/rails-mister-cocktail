<div class="container">
      <h1>Create a new cocktail</h1>
  <div class="row margin-top">
    <div class="col-sm-4 offset-sm-4">
      <%= simple_form_for @cocktail do |f| %>
        <%= f.input :name %>
        <%= f.input :photo %>
        <%= f.submit class: 'btn btn-outline-primary' %>
      <% end %>
    </div>
  </div>
</div>

